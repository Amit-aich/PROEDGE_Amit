{
  "expo": {
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
        }
      ]
    ],
    "name": "contact",
    "slug": "amit-contact",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/contactlist.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/contactlist.png",
      "resizeMode": "center",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/contactlist.png"
      },
      "package": "com.amitaich.amitcontact"
    },
    "web": {
      "favicon": "./assets/contactlist.png"
    },
    "extra": {
      "eas": {
        "projectId": "bdeee09c-d55b-44df-97de-a8601ade92a1"
      }
    }
  }
}
